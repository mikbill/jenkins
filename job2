node {  
    stage('Build') { 
        sh 'rm -rf UT' 
    }
    stage('git clone') { 
        sh 'git clone https://github.com/kadrist/UT.git'
    }
    dir("UT") {
        sh 'git checkout -b myBranch1'
    }
    dir("UT") { 
        sh 'mvn clean install'
    }
    stage('Build') { 
        sh 'cp target/devops-test.war /tmp/'
    }
}
